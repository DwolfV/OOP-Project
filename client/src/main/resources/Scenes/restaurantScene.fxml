<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<Pane fx:id="mainPane" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="nl.tudelft.oopp.demo.controllers.RestaurantSceneController"
            prefHeight="400.0" prefWidth="600" stylesheets="@/Styles/mainstyle.css">
    <HBox fx:id="hbox" prefHeight="400.0" prefWidth="600">
        <children>
            <Accordion fx:id="ac" BorderPane.alignment="CENTER">
            </Accordion>
            <Pane fx:id="pane" BorderPane.alignment="CENTER">
                <padding>
                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                </padding>
            </Pane>
        </children>
    </HBox>
</Pane>
